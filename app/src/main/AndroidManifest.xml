<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Transactpay_android_sdk"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Transactpay_android_sdk">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.transactpay.transactpay_android_sdk.Transactpay_start"
            android:label="@string/app_name" />
        <activity
            android:name="com.transactpay.transactpay_android_sdk.CardActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.transactpay.transactpay_android_sdk.Redirect_User"
            android:label="@string/app_name" />
        <activity
            android:name="com.transactpay.transactpay_android_sdk.ProcessingPage"
            android:label="@string/app_name" />
        <activity
            android:name="com.transactpay.transactpay_android_sdk.Failed"
            android:label="@string/app_name" />
        <activity
            android:name="com.transactpay.transactpay_android_sdk.BankTransfer"
            android:label="@string/app_name" />
    </application>

</manifest>
